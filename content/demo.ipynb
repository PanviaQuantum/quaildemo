    "import matplotlib.pyplot as plt\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {
    "tags": []
   },
   "outputs": [],
   "source": [
    "class quail:
   def __init__(self,filepath=None):
      self.timesteps = []    # command buffers set to Panvia Quantum Computer server
      self.debug = True
      self.programs = []     # circuits of connected qubits and quantum gates 
      self.figure = None
      self.axes = None
      self.load( filepath )

   def load(self, filepath ):
      if filepath == None:
         return
      # input JSON list of lists of commands to dict
      commands = json.load( open( filepath ))

      self.timesteps = commands

      # input commands to program creating a circuit instance      
      circuit = program( commands )

      # add the circuit to list of programs
      self.programs.append( circuit )

      self.figure = plt.figure()
      self.axes = self.figure.add_axes((0,0,1,1))

   def display(self):
      for circuit in self.programs:
         circuit.diagram(self.axes)
      plt.show()

   def states(self,x):
      state(x)

   def run(self):
      for step in self.timesteps:
         response = execute(step, panviaServer)
         print(response)
         
         action = step[0]['panvia']['command']['action']
         # no trace if store or collapse commands
         if action != 'Store' and action != 'Collapse' and  self.debug:
            data = execute( englishTrace, panviaServer) 
            state(data)
         input('press Return to continue...')
      return response\n"
   ]
  }
